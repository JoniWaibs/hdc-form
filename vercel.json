{
    "version": 2,
    "builds": [
      {
        "src": "next.config.ts",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "git": {
      "deploymentEnabled": {
        "main": true,
        "fix/*": false,
        "feature/*": false
      }
    }
  }